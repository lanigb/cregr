{
  "name": "cregr-0.01",
  "version": "0.0.1",
  "description": "a Russian grammar study tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "crergr",
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "icon512.png",
      "asar": "false"
    },
    "win": {
      "target": "portable",
      "icon": "myres/icon512.png",
      "asar": "false"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lanigb"
  },
  "keywords": [
    "declinaison",
    "russe"
  ],
  "author": "alainbe@free.fr",
  "email": "alainbe@free.fr",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "electron": "^18.3.2",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {
    "aa-sqlite": "^1.0.29",
    "electron-windows": "^18.0.0",
    "fs": "^0.0.1-security",
    "sqlite3": "^5.0.8"
  }
}
